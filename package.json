{
  "name": "pretender",
  "version": "3.0.2",
  "main": "./dist/pretender.js",
  "module": "./dist/pretender.es.js",
  "types": "index.d.ts",
  "description": "Pretender is a mock server library for XMLHttpRequest and Fetch, that comes with an express/sinatra style syntax for defining routes and their handlers.",
  "license": "MIT",
  "engines": {
    "node": "6.* || 8.* || 10.* || >= 11.*"
  },
  "scripts": {
    "prepublishOnly": "npm run build && npm run tests-only",
    "pretest": "bower install",
    "build": "rollup --config",
    "test": "npm run lint && npm run eslint && npm run tests-only",
    "test-ci": "npm run pretest && npm run build && npm run lint && npm run eslint && npm run tests-only-ci",
    "tests-only": "karma start --single-run && npm run tests-node",
    "tests-only-ci": "karma start --single-run --browsers PhantomJS && npm run tests-node",
    "tests-node": "npm run tests-iife",
    "tests-iife": "qunit test-iife/**",
    "lint": "jshint test",
    "eslint": "eslint src/**/*.ts test",
    "test:server": "karma start --no-single-run"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/pretenderjs/pretender.git"
  },
  "devDependencies": {
    "@babel/core": "^7.6.0",
    "@babel/preset-env": "^7.6.0",
    "abortcontroller-polyfill": "^1.1.9",
    "babel-jest": "^24.9.0",
    "bower": "^1.8.2",
    "coveralls": "^2.11.3",
    "es6-promise": "^4.0.5",
    "eslint": "^5.12.0",
    "jest": "^24.9.0",
    "jshint": "^2.8.0",
    "karma": "^1.7.0",
    "karma-chrome-launcher": "^2.2.0",
    "karma-coverage": "^1.1.1",
    "karma-phantomjs-launcher": "^1.0.4",
    "karma-qunit": "^2.1.0",
    "karma-sinon": "^1.0.5",
    "phantomjs": "^2.1.7",
    "qunit": "^2.6.1",
    "rollup": "^1.1.2",
    "rollup-plugin-alias": "^2.0.0",
    "rollup-plugin-multi-entry": "^2.1.0",
    "rollup-plugin-typescript": "^1.0.1",
    "sinon": "^3.2.1",
    "tslib": "^1.9.3",
    "typescript": "~3.1.1",
    "typescript-eslint-parser": "^21.0.2",
    "whatwg-fetch": "^3.0.0"
  },
  "dependencies": {
    "cross-fetch": "^3.0.4",
    "fake-xml-http-request": "^2.0.0",
    "route-recognizer": "^0.3.3"
  },
  "files": [
    "dist",
    "index.d.ts"
  ],
  "jspm": {
    "shim": {
      "pretender": {
        "deps": [
          "route-recognizer",
          "fake-xml-http-request",
          "whatwg-fetch"
        ],
        "exports": "Pretender"
      }
    }
  }
}
